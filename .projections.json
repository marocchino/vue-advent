{
  "src/components/*.vue": {
    "type": "component",
    "alternate": "src/components/__tests__/{}.test.js"
  },
  "src/components/__tests__/*.test.js": {
    "type": "test",
    "alternate": "src/components/{}.vue",
    "template": [
      "import Vue from 'vue'",
      "import {open} shallow {close} from 'vue-test-utils'",
      "import {} from '@/components/{}'",
      "describe('{}', () => {",
      "  const wrapper = shallow({})",
      "  it('renders correct contents', () => {",
      "    expect(wrapper.html()).toMatchSnapshot()",
      "  })",
      "})"
    ]
  },
  "src/api/*.js": {
    "type": "api",
    "alternate": "src/api/__tests__/{}.test.js"
  },
  "src/api/__tests__/*.test.js": {
    "type": "test",
    "alternate": "src/api/{}.js",
    "template": [
      "import {} from '@/api/{}'",
      "describe('{}', () => {",
      "})"
    ]
  }
}
